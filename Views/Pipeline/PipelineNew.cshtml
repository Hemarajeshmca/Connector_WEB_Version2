<script>
            document.addEventListener("DOMContentLoaded", function () {
                addHeaderRow();                
                const radioButtons = document.querySelectorAll('input[name="radioFilter"]');
                const formNoneDiv = document.getElementById("formnone");
                const formDataDiv = document.getElementById("formdata");
                const rawDiv = document.getElementById("raw");
                const rawOptionsDiv = document.getElementById("rawoptions");

               const rawRadio = document.getElementById('filter-raw');
                   rawRadio.checked = true;    
                   formDataDiv.style.display = "none";
                   rawDiv.style.display = "block";
                   rawOptionsDiv.style.display = "block";

                radioButtons.forEach(radio => {
                    radio.addEventListener("change", function () {
                        if (this.value === "formdata") {
                            formDataDiv.style.display = "block";
                            rawDiv.style.display = "none";
                            rawOptionsDiv.style.display = "none";
                        } else if (this.value === "raw") {
                            formDataDiv.style.display = "none";
                            rawDiv.style.display = "block";
                            rawOptionsDiv.style.display = "block";
                        } else if (this.value === "formnone") {
                            formDataDiv.style.display = "none";
                            rawDiv.style.display = "none";
                            rawOptionsDiv.style.display = "none";
                        }
                    });
                });
                // Hema
                    const urlInput = document.getElementById('url');
                    const tableBody = document.getElementById('table-body');
                function addParamRow(key = '', value = '') {
                    const tr = document.createElement('tr');
                    tr.innerHTML = `
                        <td><input type="text" class="form-control no-border key" value="${key}"></td>
                        <td><input type="text" class="form-control no-border val" value="${value}"></td>
                    `;
                    tr.querySelectorAll('input').forEach(inp => {
                        inp.addEventListener('input', () => paramChanged(inp));
                    });
                    tableBody.appendChild(tr);
                    return tr;
                }

                 function paramChanged(inputEl) {
                    const row = inputEl.closest('tr');
                    const key = row.querySelector('.key').value.trim();
                    const val = row.querySelector('.val').value.trim();
                    const lastRow = tableBody.lastElementChild;

                    if (row === lastRow && (key !== '' || val !== '')) {
                        addParamRow();
                    }

                    updateUrlFromTable();
                 }

                 function updateUrlFromTable() {
                        const base = urlInput.dataset.baseUrl || (urlInput.value.includes('?')
                          ? urlInput.value.split('?')[0]
                          : urlInput.value.split('?')[0] || '');

                        const params = new URLSearchParams();
                        Array.from(tableBody.querySelectorAll('tr')).forEach(row => {
                          const key = row.querySelector('.key').value.trim();
                          const val = row.querySelector('.val').value;
                          if (key !== '') {
                            params.set(key, val);
                          }
                        });

                        const qs = params.toString();
                        urlInput.value = qs ? `${base}?${qs}` : base;
                  }

                      // Smart URL → table parsing without breaking typing
                      let urlParseTimeout;
                      function urloninput() {
                        clearTimeout(urlParseTimeout);
                        urlParseTimeout = setTimeout(() => {
                          const full = urlInput.value.trim();
                          tableBody.innerHTML = '';

                          let base = '';
                          let queryString = '';
                          if (full.includes('?')) {
                            [base, queryString] = full.split('?');
                            const params = new URLSearchParams(queryString || '');
                            params.forEach((val, key) => addParamRow(key, val));
                          }

                          urlInput.dataset.baseUrl = base || (full && !full.includes('?') ? full : '');

                          // always one empty row
                          if (tableBody.children.length === 0) addParamRow('', '');
                          const last = tableBody.lastElementChild;
                          if (last) {
                            const k = last.querySelector('.key').value.trim();
                            const v = last.querySelector('.val').value.trim();
                            if (k !== '' || v !== '') addParamRow('', '');
                          }
                        }, 200); // debounce 200ms to avoid blocking typing
                      }

                      // Initialize table
                      urloninput();

                      // Listen to every key input / paste in URL
                      urlInput.addEventListener('input', urloninput);
                    });
      

