    function buildPayload() {
        const headers = {};
        const rows = document.querySelectorAll("#headerTableBody tr");
        rows.forEach(row => {
            const key = row.querySelector(".headerKey")?.value.trim();
            const val = row.querySelector(".headerVal")?.value.trim();
            if (key && val) {
                headers[key] = val;
            }
        });

         // Ensure Authorization is always included
        const token = $("#auth_token").val(); // assume you have an input for token
        if (token) {
            headers["Authorization"] = "Bearer " + token;
        }

        const payload = {
             url: $("#url").val(),   
             method: $("#txt_method").val(),
             body: $("#comment").val(), 
             headers: headers
        };
        return payload;
    }   

    function addHeaderRow(key = "", value = "") {
        const tableBody = document.getElementById("headerTableBody");
        const row = document.createElement("tr");
        row.innerHTML = `
            <td>
                <input type="text" class="form-control no-border headerKey"
                       value="${key}"
                       oninput="headerChanged(this)">
            </td>
            <td>
                <input type="text" class="form-control no-border headerVal"
                       value="${value}"
                       oninput="headerChanged(this)">
            </td>
        `;
        tableBody.appendChild(row);
    }

    function headerChanged(inputEl) {
        const row = inputEl.closest("tr");
        const key = row.querySelector(".headerKey").value.trim();
        const val = row.querySelector(".headerVal").value.trim();    

        const tableBody = document.getElementById("headerTableBody");
        const lastRow = tableBody.lastElementChild;

        // only add a new row if editing the last row and it has content
        if (row === lastRow && (key !== "" || val !== "")) {
            addHeaderRow();
        }
    }

    function getHeadersJson() {
        const headers = {};
        const rows = document.querySelectorAll("#headerTableBody tr");

        rows.forEach(row => {
            const key = row.querySelector(".headerKey")?.value.trim();
            const val = row.querySelector(".headerVal")?.value.trim();

            if (key && val) {
                headers[key] = val;
            }
        });

        return JSON.stringify(headers, null, 4); 
    }

