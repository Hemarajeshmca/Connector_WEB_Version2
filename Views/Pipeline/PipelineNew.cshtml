    function jsonKeyValidate(){
        const treeList = $("#TablenameGrid").data("kendoTreeList");
        const allData = treeList.dataSource.data(); // flat list (id/parentId based)
        let dt = [];
        let dtNode = [];
        let selectedKeys = [];
        // Read all rows from dataSource
        allData.forEach(item => {
            if (item.selected) {
                dt.push({
                    node: item.keyNode,
                    level: item.level,
                    parent_node: item.parentKey
                });
                selectedKeys.push(item.keyNode);
            }
        });

        if (dt.length === 0) {
            alert("Please select at least one key field to validate.");
            return;
        }
        // Find max level
        const maxLevel = Math.max(...dt.map(d => d.level));
        let keyLevel = maxLevel;
        let parentKey = "";
        let parentKey1 = "";
        let siblingKey = "";
        let childKey = "";
            // Build dtNode array (like in your C# code)
    do {
        siblingKey = "";
        let filtered = dt.filter(d => {
            if (keyLevel === maxLevel) return d.level === keyLevel;
            else return d.level === keyLevel && d.node === parentKey;
        });

        let i = 0;
        parentKey1 = parentKey;
        let data_row1 = {};

        filtered.forEach(row => {
            if (i === 0) {
                data_row1 = {
                    node: row.node,
                    parent_node: row.parent_node,
                    level: keyLevel,
                    child_node: childKey,
                    sibling: ""
                };
                childKey = row.node;
            }
            parentKey = row.parent_node;

            if (i !== 0)
            {
                siblingKey += row.node + ",";
            }
            i++;
        });

        // Add other siblings
        let others = dt.filter(d => {
            if (keyLevel === maxLevel)
                return d.level === keyLevel && d.parent_node !== parentKey;
            else
                return d.level === keyLevel && d.node !== parentKey1;
        });

        others.forEach(row => {
            siblingKey += row.node + ",";
        });

        data_row1.sibling = siblingKey.replace(/,$/, "");

        if (Object.keys(data_row1).length > 0) dtNode.push(data_row1);

        keyLevel--;
    } while (keyLevel >= 0);

         // Sort by level ascending
         dtNode.sort((a, b) => a.level - b.level);

        let message = "Validation Successful!\n\nSelected Keys:\n" +
        selectedKeys.map(k => " - " + k).join("\n");
         jAlert(message, "Connector");
    }
         
