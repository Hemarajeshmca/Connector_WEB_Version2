      function getJsonStructure(obj) {
        if (Array.isArray(obj)) {
            // Handle array → take first element as representative
            if (obj.length > 0) {
                return [getJsonStructure(obj[0])];
            } else {
                return [];
            }
        } else if (typeof obj === "object" && obj !== null) {
            // Handle objects → recurse for each key
            const structure = {};
            for (const key in obj) {
                structure[key] = getJsonStructure(obj[key]);
            }
            return structure;
        } else {
            // Primitive types → replace with empty string or type name
            return "";
        }
    }


        function extractFirstLevelKeys(jsonString) {
                let data = JSON.parse(jsonString);

                // If it's an array → take first element
                if (Array.isArray(data)) {
                    data = data[0];
                }

                // If still not an object → return empty
                if (typeof data !== "object" || data === null) {
                    return [];
                }

                // Return 1st-level keys
                return Object.keys(data);
            }

