                selectedFiles = selectedFiles.filter(f => f.uid !== uid);
            }

            if (selectedFiles.length > 0) {
                renderFileList();  
            } else {
                Supportuploadclear();
                $("#fileGrid").show();
                $("#fileGridContainer").show();
            }
        }

        function bindRemarksChangeEvent() {
            $("#fileGridContainer").off("input", ".remarks-input").on("input", ".remarks-input", function () {
                const uid = $(this).data("uid");
                const newVal = $(this).val();
                let file = selectedFiles.find(f => f.uid === uid);
                if (file) {
                    file.supportingdoc_remarks = newVal;
                    console.log(`Updated remarks for ${file.supportingdoc_name}: ${newVal}`);
                }
            });
        }

        function Submit_supportfilesupload() {
            let grid = $("#fileGridContainer").data("kendoGrid");
            if (!grid) {
                console.error("Grid not initialized");
                return;  // <-- Make sure this returns immediately if no grid
            }
            if(grid.dataSource.data().length==0){
                jAlert("Please select any one supporting File. !", "Connector");
                return;
            }
            bindRemarksChangeEvent();
            let formData = new FormData();

            let data = grid.dataSource.data(); // All rows in grid (no paging assumed)
            let allGridData = [];

            for (let i = 0; i < data.length; i++) {
                let item = data[i];
                // Get latest remarks from textbox input (if editable)
                let textbox = $(`.remarks-input[data-name='${item.supportingdoc_name}']`);
                if (textbox.length) {
                    item.supportingdoc_remarks = textbox.val().trim();
                }
                let fileObj = selectedFiles.find(f => f.uid === item.uid || f.supportingdoc_name === item.supportingdoc_name);
                allGridData.push({
                    supportingdoc_name: item.supportingdoc_name,
                    supportingdoc_remarks: item.supportingdoc_remarks,
                    supportingdoc_size: item.supportingdoc_size,
                    uid: item.uid,
                    rawFile: fileObj ? fileObj.rawFile : null
                });
            }

            // Append files to formData
            allGridData.forEach(row => {
                if (row.rawFile) {
                    formData.append("supportfileUploader", row.rawFile);
                }
            });

            // Append additional data
            _user_code = sessionStorage.getItem("userCode");
            let pipelineCode = $("#txt_ppl_code").val();

            // Add extra fields to each row data before sending as JSON
            allGridData.forEach(row => {
                row.pipeline_code = pipelineCode;
                row.created_by = _user_code;
                row.action = "insert";
            });

            formData.append("gridDataJson", JSON.stringify(allGridData));
            formData.append("pipeline_code", pipelineCode);
            formData.append("created_by", _user_code);
            formData.append("action", "insert");

            $.ajax({
                url: '@Url.Action("UploadSupportFiles", "Pipeline")',
                type: "POST",
                processData: false,
                contentType: false,
                data: formData,
                success: function (response) {
                    GetSupportDocs();
                    jAlert("Upload success", "Connector");
                    Supportuploadclear();
                },
                error: function (xhr) {
                    console.error("Upload error", xhr);
                }
            });
        }

        function updateGridDataFromInputs() {
            $("#fileGrid").show();
            $("#fileGridContainer").show();
            const grid = $("#fileGridContainer").data("kendoGrid");
            const data = grid.dataSource.data();

            $("#fileGridContainer").find(".remarks-input").each(function () {
                const uid = $(this).data("uid");
                const value = $(this).val();
                const item = data.find(d => d.uid === uid);
                if (item) {
                    item.set("supportingdoc_remarks", value);
                }
            });
        }


